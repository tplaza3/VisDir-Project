<div id="container">
	
  <div id="header">
    <div id="headerguts">
    	<h1>Task, Inc.</h1>
  	</div>
  </div>
	
  <div id="sidebar">
    <div id="sidebarguts">
    	<h2>User Log In</h2>
    		<p>Not an existing user?</p>
	    <div id="sidebarindent">
		    <p> <%= link_to("Create User Account", signup_path) %> </p>
		    <p><%= link_to 'Return to Home', root_path %></p>
    	</div>
  	</div>
  </div>
	
	<div id="contentbig">
		<div id="contentbigguts">
			<div id="contentbigguts">	
				
				<p>
		     	<%= form_tag login_path do %>
		     <div class="field">
		      <%= label_tag :email %><br />
		      <%= text_field_tag :email, params[:email] %>
		   	 </div>
		 	 	</p>
	
		    <p>
		      <div class="field">
		        <%= label_tag :password %><br />
		        <%= password_field_tag :password %>
		      </div>
		    </p>
		     		<%= submit_tag "Log In" %>
		        <% end %>
						
			</div>
		</div>
	</div>
	
  <div id="clearcontentright">
  </div>
	
  <div id="footer">
  </div>

</div>

